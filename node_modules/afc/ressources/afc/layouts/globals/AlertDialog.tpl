        <div id="parentPanel">
            <div id="topPanel"></div>
            <afc-gui-div id="contentPanel">
                <div id="custom">
                    <div id="message"></div>
                </div>
            </afc-gui-div>
            <div id="buttonPanel">
                <div class="filler">
